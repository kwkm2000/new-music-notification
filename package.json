{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "pm2-runtime start dist/src/index.js",
    "stop": "pm2 stop 0",
    "restart": "pm2 restart 0",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "test": "node test.js",
    "deploy": "yarn build && gcloud app deploy",
    "crawl:single": "node dist/src/crawler.js",
    "crawl:artist": "node dist/src/artist_crawler.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "express": "^4.15.2",
    "node-fetch": "^2.6.1",
    "pm2": "^3.0.2",
    "puppeteer": "^5.1.0",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.7",
    "@types/node": "^14.0.23",
    "@types/express": "^4.17.7",
    "@types/puppeteer": "^3.0.1",
    "got": "^11.3.0",
    "tape": "^4.7.0",
    "ts-node": "^8.10.2"
  },
  "keywords": [
    "node",
    "express"
  ],
  "license": "MIT"
}
