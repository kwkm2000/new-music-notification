{
  "compilerOptions": {
    "target": "es5",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"],
      "@public/*": ["public/*"]
    }
  },
  "include": [
    "src/**/*",
  ],
"exclude": ["node_modules"]
}
